{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "ganeshswagruhafoods.com"
        }
      ],
      "headers": [
        { "key": "Content-Encoding", "value": "br" }
      ],
      "destination": "https://www.ganeshswagruhafoods.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/((?!sitemap\\.xml).*)",
      "destination": "/"
    }
  ]
}
